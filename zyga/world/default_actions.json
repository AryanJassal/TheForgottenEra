{
    "actions": {
        "go|move": {
            "left": "$move_left",
            "right": "$move_right",
            "up": "$move_up",
            "down": "$move_down"
        },
        "use": {
            "%consumable": "$use"
        },
        "display|see|view|open": {
            "stats": "$stats"
        },
        "stats": "$stats",
        "exit|quit": "$quit"
    }
}