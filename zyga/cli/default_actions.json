{
    "actions": {
        "go|move": {
            "left": "$move_left",
            "right": "$move_right",
            "up": "$move_up",
            "down": "$move_down"
        },
        "use": {
            "%consumable": "$use"
        },
        "display|see|view|open": {
            "stats": "$stats",
            "inv|inventory": "$inventory"
        },
        "stats": "$stats",
        "inv|inventory": "$inventory",
        "cls|clear": "$clear_screen",
        "exit|quit": "$quit"
    }
}